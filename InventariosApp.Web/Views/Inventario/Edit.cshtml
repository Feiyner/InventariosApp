@model InventariosApp.Data.Inventario

<h2>Edit Product</h2>

<form asp-action="Edit">
    <div class="form-group">
        <label asp-for="ProductId"></label>
        <input asp-for="ProductId" class="form-control" />
    </div>
    <div class="form-group">
        <label asp-for="ProductPrice"></label>
        <input asp-for="ProductPrice" class="form-control" />
    </div>
    <div class="form-group">
        <label asp-for="ProductName"></label>
        <input asp-for="ProductName" class="form-control" />
    </div>
    <div class="form-group">
        <label asp-for="Motivo"></label>
        <input asp-for="Motivo" class="form-control" />
    </div>
    <div class="form-group">
        <label asp-for="Cantidad"></label>
        <input asp-for="Cantidad" class="form-control" />
    </div>
    <div class="form-group">
        <label asp-for="Proveedor"></label>
        <input asp-for="Proveedor" class="form-control" />
    </div>
    <div class="form-group">
        <label asp-for="FechaTransaccion"></label>
        <input asp-for="FechaTransaccion" class="form-control" />
    </div>
    <div class="form-group">
        <label asp-for="PeriodoCerrado"></label>
        <input asp-for="PeriodoCerrado" class="form-control" />
    </div>
    <div class="form-group">
        <label asp-for="TipoMovimiento"></label>
        <input asp-for="TipoMovimiento" class="form-control" />
    </div>
    <div class="form-group">
        <label asp-for="NroDocumento"></label>
        <input asp-for="NroDocumento" class="form-control" />
    </div>
    <button type="submit" class="btn btn-primary">Save</button>
</form>
